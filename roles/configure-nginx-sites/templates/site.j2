server {
	listen 80;
	listen [::]:80;
	
  root /home/deployer/web/public;
	index index.html index.htm index.php;
	
  server_name test.kanshoubakuya.com;

	location / {
		try_files $uri $uri/ /index.php$is_args$args;
	}
	
	location ~ \.php$ {
		fastcgi_pass unix:/var/run/php7.1/deployer.sock;
		fastcgi_index index.php;
		include fastcgi_params;
	}
}